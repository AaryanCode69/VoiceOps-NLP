{
  "name": "n8n-nodes-voiceops",
  "version": "0.1.0",
  "description": "n8n community node for VoiceOps fraud detection analysis",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "voiceops",
    "fraud-detection"
  ],
  "license": "MIT",
  "homepage": "https://github.com/yourusername/n8n-nodes-voiceops",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/n8n-nodes-voiceops.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "copyfiles -u 1 \"src/**/*.{json,svg,png}\" dist/",
    "dev": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/VoiceOpsApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/VoiceOpsAnalyzeCall/VoiceOpsAnalyzeCall.node.js",
      "dist/nodes/RiskAnalyzer/RiskAnalyzer.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.16.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.40.0",
    "fast-check": "^3.15.0",
    "jest": "^29.5.0",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {},
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
